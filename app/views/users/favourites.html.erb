<div class="table-responsive">
	<table class="table table-hover">
		<tr>
			<th>Name</th>
			<th>Year</th>
			<th>Action</th>
		</tr>
		<% @fav_posts.each do |post| %>
			<tr>
				<td><%= post.name %></td>
				<td><%= post.year %></td>
				<td><%= link_to "Show", post_path(post) %>
			</tr>
		<% end %>
	</table>
</div>